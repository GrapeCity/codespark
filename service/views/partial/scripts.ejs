    (function ($) {
        $.extend({
            loading: {
                show: function () {
                    $('.progress-container').removeClass('hide');
                },
                hide: function () {
                    $('.progress-container').addClass('hide');
                }
            }
        });
    })(jQuery);

    $().ready(function () {

        // the body of this function is in assets/material-kit.js
